{
	"$schema": "vscode://schemas/color-theme",
	"name": "Skipper Theme",
	"type": "dark",
	"colors": {
	  "editor.background": "#1e2030",
	  "editor.foreground": "#EEFFFF",
	  "activityBar.background": "#1a1b26",
	  "sideBar.background": "#1a1b26",
	  "sideBar.foreground": "#a9b1d6",
	  "titleBar.activeBackground": "#1a1b26",
	  "titleBar.activeForeground": "#a9b1d6",
	  "titleBar.inactiveBackground": "#1a1b26",
	  "titleBar.inactiveForeground": "#565f89",
	  "statusBar.background": "#1a1b26",
	  "statusBar.foreground": "#a9b1d6",
	  "statusBar.noFolderBackground": "#1a1b26",
	  "statusBar.debuggingBackground": "#1a1b26",
	  "editorGroupHeader.tabsBackground": "#1a1b26",
	  "tab.inactiveBackground": "#1a1b26",
	  "tab.activeBackground": "#1e2030",
	  "tab.activeForeground": "#a9b1d6",
	  "tab.inactiveForeground": "#565f89",
	  "editorLineNumber.foreground": "#546E7A",
	  "editorLineNumber.activeForeground": "#a9b1d6",
	  "editor.lineHighlightBackground": "#292e42",
	  "editorIndentGuide.background": "#37474F80",
	  "editorIndentGuide.activeBackground": "#80CBC470",
	  "list.activeSelectionBackground": "#292e42",
	  "list.hoverBackground": "#292e42",
	  "scrollbarSlider.background": "#292e4280",
	  "scrollbarSlider.hoverBackground": "#3b4261",
	  "terminal.background": "#1e2030",
	  "terminal.foreground": "#EEFFFF"
	},
	"tokenColors": [
		{
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"scope": ["string", "constant.other.symbol", "entity.other.inherited-class"],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"scope": ["variable", "variable.language"],
			"settings": {
				"foreground": "#EEFFFF"
			}
		},
		{
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"scope": ["entity.name.function", "meta.function-call", "support.function"],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"scope": ["entity.name.type", "entity.name.class", "entity.other.attribute-name"],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"scope": ["entity.name.tag", "punctuation.definition.tag"],
			"settings": {
				"foreground": "#F07178"
			}
		},
		{
			"scope": ["punctuation", "meta.brace"],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"scope": ["markup.inserted"],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"scope": ["markup.deleted"],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"scope": ["markup.changed"],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"scope": ["string.regexp"],
			"settings": {
				"foreground": "#89DDFF"
			}
		}
	]
}