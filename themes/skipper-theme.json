{
	"$schema": "vscode://schemas/color-theme",
	"name": "Skipper Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#373a4b",
		"editor.foreground": "#cacbdc",
		"activityBar.background": "#373a4b",
		"activityBar.foreground": "#cacbdc",
		"sideBar.background": "#373a4b",
		"sideBar.foreground": "#cacbdc",
		"titleBar.activeBackground": "#424256",
		"titleBar.activeForeground": "#cacbdc",
		"titleBar.inactiveBackground": "#424256",
		"titleBar.inactiveForeground": "#8e8fa4",
		"statusBar.background": "#424256",
		"statusBar.foreground": "#cacbdc",
		"statusBar.noFolderBackground": "#424256",
		"statusBar.debuggingBackground": "#424256",
		"editorGroupHeader.tabsBackground": "#424256",
		"tab.inactiveBackground": "#424256",
		"tab.activeBackground": "#373a4b",
		"tab.activeForeground": "#cacbdc",
		"tab.inactiveForeground": "#8e8fa4",
		"editorLineNumber.foreground": "#8e8fa4",
		"editorLineNumber.activeForeground": "#cacbdc",
		"editor.lineHighlightBackground": "#52536f",
		"editorIndentGuide.background": "#52536f80",
		"editorIndentGuide.activeBackground": "#cacbdc70",
		"list.activeSelectionBackground": "#52536f",
		"list.hoverBackground": "#52536f",
		"scrollbarSlider.background": "#52536f80",
		"scrollbarSlider.hoverBackground": "#52536f",
		"terminal.background": "#373a4b",
		"terminal.foreground": "#cacbdc",
		"panel.background": "#373a4b",
		"panel.border": "#424256",
		"panelTitle.activeForeground": "#cacbdc",
		"panelTitle.inactiveForeground": "#8e8fa4",
		"dropdown.background": "#373a4b",
		"dropdown.foreground": "#cacbdc",
		"input.background": "#373a4b",
		"input.foreground": "#cacbdc",
		"button.background": "#52536f",
		"button.foreground": "#cacbdc"
	},
	"tokenColors": [
		{
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"scope": ["string", "constant.other.symbol", "entity.other.inherited-class"],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"scope": ["variable", "variable.language"],
			"settings": {
				"foreground": "#EEFFFF"
			}
		},
		{
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"scope": ["entity.name.function", "meta.function-call", "support.function"],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"scope": ["entity.name.type", "entity.name.class", "entity.other.attribute-name"],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"scope": ["entity.name.tag", "punctuation.definition.tag"],
			"settings": {
				"foreground": "#F07178"
			}
		},
		{
			"scope": ["punctuation", "meta.brace"],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"scope": ["markup.inserted"],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"scope": ["markup.deleted"],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"scope": ["markup.changed"],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"scope": ["string.regexp"],
			"settings": {
				"foreground": "#89DDFF"
			}
		}
	]
}